.weui-help{
    display: flex;
    justify-content: center;
    color: #fe9728;
    text-align: center;
}

.circle_mid {
    background-color:#fff;
    opacity: .2;
    width: 100px;
    height: 100px;
    left: 50%;
    border-radius: 50%;
    transform: translateX(-50%);
    position: absolute;
    top: 13px;
}

.jx_cash{
    width: 76px;
    height: 76px;
    z-index: 500;
    margin: 58px auto;
    padding-left: 2px;
}


.weui-msg__title {

    font-size: 16px;
    color: #8a8a8a;
    padding-top: 25px;

}