<view class="page" xmlns:wx="http://www.w3.org/1999/xhtml">





     <view class="weui-cells weui-cells_after-title ">
        <image src="../../../static/icon/wages/jx_logo_orange.png" class="jx_logo"/>
     </view>


     <view class="weui-cells weui-cells_after-title">

        <view class="weui-cell weui-cell_input">
            <view class="weui-cell__hd">
                <view class="weui-label">手机</view>
            </view>

            <view class="weui-cell__bd">
                <input class="weui-input" value="{{mobile}}" bindinput="telFn" placeholder="请输入您的手机号"/>
            </view>
        </view>

     </view>


     <view class="weui-cells weui-cells_after-title">

        <view class="weui-cell weui-cell_input">
            <view class="weui-cell__hd">
                <view class="weui-label">密码</view>
            </view>
            <view class="weui-cell__bd">
                <input class="weui-input" password='true' value="{{password}}" bindinput="passwordFn" placeholder="请输入密码"/>
            </view>
        </view>

    </view>


     <view class="page__bd page__bd_spacing">
        <button class="weui-btn" type="primary" bindtap="signin" style="background: -webkit-linear-gradient(left bottom,#E87B17,#eb5e26); font-size: 14px;">登录</button>


     </view>

     <view class="weui-help">

        <navigator  url="../forget_password/forget_password"  class="weui-media-box__title">忘记密码？</navigator>

        <navigator  url="../register/register"  class="weui-media-box__title">免费注册</navigator>

     </view>

</view>